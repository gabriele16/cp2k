&FORCE_EVAL
  METHOD Quickstep
  &PROPERTIES
    &ATOMIC
      ENERGY T
    &END
  &END PROPERTIES
  &DFT
    BASIS_SET_FILE_NAME BASIS_SET
    POTENTIAL_FILE_NAME GTH_POTENTIALS
    &MGRID
      CUTOFF 200
    &END MGRID
    &QS
      EPS_DEFAULT 1.E-10
    &END QS
    &SCF
      EPS_SCF 1.0E-5
      SCF_GUESS ATOMIC  
    &END
    &XC
      &XC_FUNCTIONAL PADE
      &END
    &END XC
    &PRINT
      &ENERGY_DECOMPOSITION_ANALYSIS
      &END ENERGY_DECOMPOSITION_ANALYSIS
    &END PRINT
  &END DFT
  &SUBSYS
    &CELL
      ABC 10.0 10.0 10.0
    &END CELL
    &COORD
    O     0.000000     0.000000     0.000000    H2O1
    H     0.000000     0.000000     1.000000    H2O1
    H     0.942809     0.000000    -0.333333    H2O1
    O    -1.617979    -0.948062    -2.341650    H2O2
    H    -2.529195    -1.296822    -2.122437    H2O2
    H    -1.534288    -0.833088    -3.331486    H2O2
    &END COORD
    &KIND H
      BASIS_SET ORB DZVP-GTH-BLYP
      POTENTIAL GTH-PADE-q1
      MAO 1
    &END KIND
    &KIND O
      BASIS_SET ORB DZVP-GTH-BLYP
      POTENTIAL GTH-PADE-q6
      MAO 4
    &END KIND
  &END SUBSYS
&END FORCE_EVAL
&GLOBAL
  PROJECT wdim
  RUN_TYPE ENERGY
  PRINT_LEVEL LOW
&END GLOBAL
